<h1>Listing deletemes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Company</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @deletemes.each do |deleteme| %>
  <tr>
    <td><%= deleteme.title %></td>
    <td><%= deleteme.company %></td>
    <td><%= deleteme.description %></td>
    <td><%= link_to 'Show', deleteme %></td>
    <td><%= link_to 'Edit', edit_deleteme_path(deleteme) %></td>
    <td><%= link_to 'Destroy', deleteme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

